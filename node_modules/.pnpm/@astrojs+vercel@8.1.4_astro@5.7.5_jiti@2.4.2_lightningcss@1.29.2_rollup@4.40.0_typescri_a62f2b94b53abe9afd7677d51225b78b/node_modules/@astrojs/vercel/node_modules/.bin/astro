#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/inarty/Portfolio/astro/nik-tes/node_modules/.pnpm/astro@5.7.5_jiti@2.4.2_lightningcss@1.29.2_rollup@4.40.0_typescript@5.8.3/node_modules/astro/node_modules:/home/inarty/Portfolio/astro/nik-tes/node_modules/.pnpm/astro@5.7.5_jiti@2.4.2_lightningcss@1.29.2_rollup@4.40.0_typescript@5.8.3/node_modules:/home/inarty/Portfolio/astro/nik-tes/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/inarty/Portfolio/astro/nik-tes/node_modules/.pnpm/astro@5.7.5_jiti@2.4.2_lightningcss@1.29.2_rollup@4.40.0_typescript@5.8.3/node_modules/astro/node_modules:/home/inarty/Portfolio/astro/nik-tes/node_modules/.pnpm/astro@5.7.5_jiti@2.4.2_lightningcss@1.29.2_rollup@4.40.0_typescript@5.8.3/node_modules:/home/inarty/Portfolio/astro/nik-tes/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../astro@5.7.5_jiti@2.4.2_lightningcss@1.29.2_rollup@4.40.0_typescript@5.8.3/node_modules/astro/astro.js" "$@"
else
  exec node  "$basedir/../../../../../../astro@5.7.5_jiti@2.4.2_lightningcss@1.29.2_rollup@4.40.0_typescript@5.8.3/node_modules/astro/astro.js" "$@"
fi
